{
  "name": "z6chat",
  "version": "0.3.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:convex": "npx convex dev",
    "build": "next build && npx convex deploy",
    "build:ci": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality:check": "pnpm type-check && pnpm lint && pnpm format:check",
    "quality:fix": "pnpm lint:fix && pnpm format",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "convex:deploy": "npx convex deploy",
    "convex:import": "npx convex import",
    "convex:dashboard": "npx convex dashboard",
    "changeset": "changeset",
    "changeset:add": "./scripts/safe-commands.sh add",
    "changeset:generate": "./scripts/safe-commands.sh generate",
    "changeset:version": "./scripts/safe-commands.sh version",
    "changeset:publish": "changeset publish",
    "changeset:status": "./scripts/safe-commands.sh status",
    "release": "pnpm changeset:version && git add . && git commit -m 'chore: version packages' && git push",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "prismjs": ">=1.30.0"
    }
  },
